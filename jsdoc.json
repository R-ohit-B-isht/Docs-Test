{
    "source": {
        "include": [".", "package.json", "README.md"],
        "exclude": ["node_modules", "docs", "test", "coverage", "dist","Rodic-HRMS-Backend-Components"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "opts": {
        "encoding": "utf8",
        "readme": "./README.md",
        "tutorials": "./tutorials",
        "destination": "./docs",
        "recurse": true,
        "verbose": true,
        "template": "./node_modules/clean-jsdoc-theme",
        "theme_opts": {
            "menu": [
                {
                    "title": "Backend-Components",
                    "link": "./Rodic-HRMS-Backend-Components/docs/rodic-hrms-backend-components/0.0.0/index.html"
                }
              ],
            "static_dir": ["./images","./Rodic-HRMS-Backend-Components"],
            "default_theme": "dark" ,
            "displayModuleHeader": true,
            "search": true,
            "sort": false,
            "homepageTitle": "Clean JSDoc theme",
            "includeFilesListInHomepage": true,
            "sections": ["Classes", "Modules", "Externals", "Events", "Namespaces", "Mixins", "Tutorials", "Interfaces", "Global"]
        }
    },
    "markdown": {
        "hardwrap": false,
        "idInHeadings": true
    }
  }
  